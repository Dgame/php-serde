{
    "name": "dgame/php-serde",
    "type": "library",
    "description": "",
    "keywords": [],
    "homepage": "https://github.com/Dgame/php-serde",
    "license": "MIT",
    "authors": [
        {
            "name": "Randy Sch√ºtt",
            "email": "rswhite4@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.4",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.4",
        "spaceemotion/php-coding-standard": "dev-wip/v1"
    },
    "config": {
        "platform": {
            "php": "8.0"
        },
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Dgame\\Serde\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dgame\\Serde\\Test\\": "test/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "analyse": "phpcstd",
        "coverage": "phpunit --coverage-clover=coverage",
        "test": "phpunit"
    }
}
